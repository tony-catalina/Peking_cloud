<div class="page navbar-fixed toolbar-fixed">
	<div class="page-content" style="padding-top: 0px;">
		<div class="navbar">
			<div class="navbar-inner">
				<div class="left" style="display: inline-block">
          			<a href="javascript:void(0);" class="back link">
            			<i class="icon icon-back"></i>
            			<span style="padding-top: 3px">发起上门</span>
          			</a>
			</div>
			<div>
				<a href="#" id="addsm" class="button button-round active">新增上门</a>
        	</div>
		</div>
    </div>
    <div class="list-block" style="margin: 0 0;">
		<ul>
        {{#each data}}
			<li class="accordion-item">
				<a href="#" class="item-content item-link">
					<div class="item-inner">
						<div class="item-title">{{@key}}</div>
					</div>
				</a>
				<div class="accordion-item-content">
      				<div class="content-block">
                		<div class="list-block">
                  			<ul>
                    		{{#each this}}
 								<li class="swipeout">
                        			<div class="swipeout-content item-content">
                          				<div class="item-media"><i class="f7-icons color-blue">person</i></div>
                          				<div class="item-inner">
                            				<div class="item-title" uuid={{uuid}} >{{xyrxm}}({{filenum}})</div>
                          				</div>
                       		 		</div>                        
                        			<div class="swipeout-actions-right">
                          				<a href="javascript:void(0);" dataid="{{id}}" class="action2 bg-red">取消上门</a>
                        			</div>
                      			</li>
           					{{/each}}
                  			</ul>
                		</div>
            		</div>
				</div>
			</li>
		{{/each}}
		</ul>
	</div>
</div>
