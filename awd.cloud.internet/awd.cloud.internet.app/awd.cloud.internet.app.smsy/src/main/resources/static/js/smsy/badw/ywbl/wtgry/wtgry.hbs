<div class="page toolbar-fixed">
	<div class="page-content" style="padding-top: 0px;">
		<div class="navbar">
			<!-- 导航条-->
			<div class="navbar-inner">
					<a href="javascript:void(0);" class="link back">
						<i class="icon icon-back"></i>
						<span style="padding-top: 3px">未通过人员</span>
					</a>
			</div>
		</div>
		<div class="list-block " style="margin-top: 0px">
			<div class="list-block accordion-list" style="width:100%; margin:0 0;">
				<ul>
					{{#each data}}
						<li class="accordion-item"><a href="#" class="item-content item-link">
								<div class="item-inner">
									<div class="item-title">{{@key}}</div>
								</div>
							</a>
							<div class="accordion-item-content">
								<div class="content-block">
									<div class="list-block">
										<ul>
											{{#each this}}
												<li class="swipeout">
													<div class="swipeout-content item-content">
														<div class="item-media"><i class="f7-icons color-blue">person</i></div>
														<div class="item-inner">
															<div class="item-title" uuid="{{uuid}}">{{xyrxm}}({{filenum}})</div>
															<div class="item-after" style="font-size:10px;">{{updatetime}}</div>
														</div>
													</div>
													<div class="swipeout-actions-right">
														<a href="javascript:void(0);" dataid="{{id}}" 
															xyrxm="{{xyrxm}}"  gyjs="{{gyjs}}" gyjsString="{{gyjsString}}" 
																class="action1 bg-green" style="padding:0 20px">
																<p style="font-size:16px;">完善信息</p></a>
														<a href="javascript:void(0);" dataid="{{id}}" class="action2 bg-red" style="padding:0 20px"><p style="font-size:16px;">撤销上门</p></a>
														<a href="javascript:void(0);" jsly="{{jsly}}" class="action3 bg-blue" style="padding:0 20px"><p style="font-size:16px;">拒收原因</p></a>
													</div>
												</li>
											{{/each}}
										</ul>
									</div>
								</div>
							</div>
						</li>
					{{/each}}
				</ul>
			</div>
		</div>
	</div>
</div>
